---
import Card from "./Card.astro";
const response = await fetch('https://codeforces.com/api/user.rating?handle=CognusBoi');
const data = await response.json();
const latestRating = data.result[data.result.length - 1].newRating;
---

<div class="flex flex-col items-center">
  <a href="https://codeforces.com/profile/CognusBoi">
    <h1 class="pt-0">
      Codeforces rating: 
      <span class="text-nord7">
       {latestRating} 
      </span>
    </h1>
  </a>
</div>
